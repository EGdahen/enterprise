<% if (section.params.length && section.index === '0') { %>
  <h2>Settings</h2>
  <table class="section-table">
    <thead>
      <th>Name</th>
      <th>Type</th>
      <th>Description</th>
    </thead>
    <tbody><% section.params.forEach(function(param) { %><% if ((section.kind === 'class' && param.name !== 'element' && param.name !== 'settings')) { %>
      <tr>
        <td>
          <%- param.name %><% if (param.default) { %><span><%- param.default %></span><% } %>
        </td>
        <td>
          <%= typeNoLink(param.type) %>
        </td>
        <td>
          <%= md(param.description, true) %>
        </td>
      </tr><% } %><% }) %>
    </tbody>
  </table>
<% } %>
<% if (section.properties.length && section.index === '0') { %>
  <h2>Settings</h2>
  <table class="section-table">
    <thead>
      <th>Name</th>
      <th>Type</th>
      <th>Description</th>
    </thead>
    <tbody><% section.properties.forEach(function(prop) { %>
      <tr>
        <td>
          <%- prop.name %><% if (prop.default) { %><span><%- prop.default %></span><% } %>
        </td>
        <td>
          <%= typeNoLink(prop.type) %>
        </td>
        <td>
          <%= md(prop.description, true) %>
        </td>
      </tr><% }) %>
    </tbody>
  </table>
<% } %>
<% if (section.examples.length) { %>
  <h2>Example</h2><% section.examples.forEach(function(example) { %><% if (example.caption) { %><p><%= md(example.caption) %></p><% } %>
    <pre><%= highlight(example.description) %></pre><% }) %><% } %><% if (section.members.instance && section.members.instance.length) { %>
  <h2>Methods</h2>
  <table class="section-table">
    <thead>
      <th>Name</th>
      <th>Parameters</th>
      <th>Description</th>
      <th>Returns</th>
    </thead>
    <tbody><% section.members.instance.forEach(function(method) { %>
      <tr>
        <td><%= method.name %></td>
        <td>
          <% if (method.params.length) { %>
                <% method.params.forEach(function(param) { %>
                  <span><%- param.name %> (<%= typeNoLink(param.type) %>) = <%= md(param.description, true) %> </span>
                <% }) %>
          <% } %>
        </td>
        <td>
          <%= md(method.description) %>
        </td>
        <td><% if (method.returns.length) { %><% method.returns.forEach(function(ret) { %><% if (ret.description) { %>
          <%= md(ret.description, true) %><%= typeNoLink(ret.type) %><% }%><% }) %><% } %>
        </td>
      </tr><% }) %>
    </tbody>
  </table>
<% } %>
