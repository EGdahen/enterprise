<tr>
  <td><%- event.name %>
  </td>
  <td>
  <% if (event.properties.length) { %>
    <% event.properties.forEach(function(property) { %>
      <div>
        <span><%- property.name%> (<%= typeNoLink(property.type) %>)
        <% if (property.default) { %>
          (default <%- property.default %>)
        <% } %>
        <% if (property.description) {
          %>= <%= md(property.description, true) %><%
        } %>
        </span>
        <% if (property.properties && property.properties.length) { %>
          <ul>
            <% property.properties.forEach(function(property) { %>
              <li><code><%- property.name %></code> <%= typeNoLink(property.type) %>
                <% if (property.default) { %>
                  (default <code><%- property.default %></code>)
                <% } %>
                <%= md(property.description) %></li>
            <% }) %>
          </ul>
        <% } %>
      </div>
    <% }) %>
  <% } %>
  </td>
  <td><%= md(event.description, true) %></td>
</tr>
