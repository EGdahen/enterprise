<% if (setting.title === 'param') { %>
  <tr>
    <td>
      <%- setting.name %>
    </td>
    <td>
      <% if (setting.default) { %><span><%- setting.default %></span><% } %>
    </td>
    <td>
      <%= typeNoLink(setting.type) %>
    </td>
    <td>
      <%= setting.description %>
    </td>
  </tr>
<% } %>
