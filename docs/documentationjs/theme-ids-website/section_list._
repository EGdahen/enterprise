<div class="jsdoc-functions">
  <% members.forEach(function(member) { %>
    <div id='<%- slug(member.namespace) %>'>
      <div class="jsdoc-functions--heading">
        <%= shortSignature(member) %>
      </div>
      <div class="jsdoc-functions--content">
        <%= renderSection({
          section: member,
          renderSection: renderSection,
          renderParamProperty: renderParamProperty,
     	  nested: true
        }) %>
      </div>
    </div>
  <% }) %>
</div>
