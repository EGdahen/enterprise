{
  "name": "@infor/sohoxi",
  "version": "4.2.0",
  "shortName": "sohoxi",
  "description": "Hook & Loop - SoHo XI Controls",
  "author": "Hook & Loop",
  "publishConfig": {
    "scope": "@infor",
    "registry": "http://npm.infor.com:4873/"
  },
  "scripts": {
    "start": "node server.js",
    "stop": "./bin/stop.sh",
    "eslint": "eslint src -f node_modules/eslint-bamboo-formatter/reporter.js",
    "build": "grunt",
    "install-test-deps": "npm install selenium-standalone@latest && npm run install-drivers",
    "install-drivers": "./node_modules/.bin/selenium-standalone install --version=2.53.0 --baseURL=http://selenium-release.storage.googleapis.com --drivers.chrome.version=2.21 --drivers.chrome.baseURL=http://chromedriver.storage.googleapis.com --drivers.ie.arch=x64 --drivers.ie.baseURL=http://selenium-release.storage.googleapis.com",
    "selenium": "./node_modules/.bin/selenium-standalone start",
    "watch": "grunt watch",
    "test": "./bin/test-build.sh",
    "test:functional": "./bin/test.sh runner config=test2/intern.local.functional",
    "test:unit": "./bin/test.sh client config=test2/intern.local.unit"
  },
  "main": "sohoxi.js",
  "dependencies": {},
  "devDependencies": {
    "chai": "^1.10.0",
    "chai-jquery": "^1.2.3",
    "colors": "^0.6.2",
    "connect-multiparty": "^2.0.0",
    "express": "^4.13.4",
    "express-slash": "^2.0.1",
    "extend": "^3.0.0",
    "grunt": "^0.4.5",
    "grunt-banner": "^0.3.1",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compress": "^0.14.0",
    "grunt-contrib-concat": "^0.4.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-cssmin": "^0.11.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-uglify": "^0.8.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-git-revision": "0.0.1",
    "grunt-mocha-cli": "^1.14.0",
    "grunt-nodemon": "^0.4.2",
    "grunt-run": "^0.3.0",
    "grunt-sass": "^1.2.0",
    "grunt-shell": "^1.3.0",
    "grunt-strip-code": "^0.1.2",
    "intern": "^3.2.0",
    "jquery": "^3.0.0-beta1",
    "jsdom": "^3.1.2",
    "jshint": "^2.9.2",
    "load-grunt-tasks": "^0.6.0",
    "mmm": "^0.2.2",
    "mocha": "^1.21.5",
    "phantomjs": "^1.9.20",
    "requirejs": "^2.2.0",
    "selenium-standalone": "^5.1.0",
    "spec-xunit-file": "0.0.1-3"
  }
}
