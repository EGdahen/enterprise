{
  "name": "@infor/sohoxi",
  "version": "4.4.0-rc",
  "shortName": "sohoxi",
  "description": "Hook & Loop - SoHo XI Controls",
  "author": "Hook & Loop",
  "license": "Runtime License",
  "repository": {
    "type": "git",
    "url": "git://git.infor.com/scm/soho/controls.git"
  },
  "publishConfig": {
    "scope": "@infor",
    "registry": "http://npm.infor.com:4873/"
  },
  "scripts": {
    "start": "node server.js",
    "stop": "./bin/stop.sh",
    "jshint": "jshint gruntfile.js app.js components/**/*.js",
    "eslint": "eslint -c .eslintrc.js components",
    "eslint:log": "eslint -c .eslintrc.js components -o eslint.log",
    "eslint:error-only": "eslint -c .eslintrc.js --quiet components",
    "build": "rollup -c",
    "rollup": "rollup",
    "install-test-deps": "npm install selenium-standalone@latest && npm run install-drivers",
    "install-drivers": "selenium-standalone install --config=./build/configs/selenium-standalone-install.js",
    "selenium": "selenium-standalone start",
    "watch": "grunt watch",
    "grunt": "grunt",
    "karma": "karma",
    "test": "./bin/test-build.sh",
    "test:functional": "./bin/test.sh runner config=test/intern.local.functional",
    "test:unit": "./bin/test.sh client config=test/intern.local.unit",
    "test:es6:unit": "npm run grunt && karma start karma.conf.js",
    "test:es6-browserstack:unit": "karma start karma.browserstack.conf.js",
    "pree2elocal": "npm run webdriver && (npm run start & sleep 10)",
    "e2elocal": "protractor ./protractor.conf.js || npm run stop",
    "poste2elocal": "npm run stop && exit 0",
    "pree2e": "npm run webdriver && (npm run start & sleep 10)",
    "e2e": "protractor ./protractor.browserstack.conf.js || npm run stop",
    "poste2e": "npm run stop && exit 0",
    "pree2emulti": "npm run webdriver && (npm run start & sleep 10)",
    "e2emulti": "protractor ./protractor.multiple.browserstack.conf.js || npm run stop",
    "poste2emulti": "npm run stop && exit 0",
    "webdriver": "webdriver-manager update --standalone false --gecko false --quiet",
    "webdriver:update": "webdriver-manager update",
    "develop-tag": "./bin/add-npm-tag.js develop",
    "bump-version": "./bin/bump-version.js",
    "publish-dev": "grunt publish && npm publish publish --tag dev --force",
    "publish-prod": "grunt publish && npm publish publish --force",
    "install-globals": "npm install -g pandoc-bin grunt-cli documentation rollup",
    "documentation": "./build/generate-documentation.js",
    "publish-aws": "AWS_PROFILE=sohoxi directory-to-s3 -d publish infor-devops-core-soho-us-east-1/sohoxi/4.3.3 -v"
  },
  "dependencies": {
    "browserstack-local": "^1.3.0",
    "d3": "^4.12.2",
    "jquery": "^3.1.1",
    "r2": "^2.0.0"
  },
  "main": "dist/js/sohoxi",
  "jspm": {
    "main": "dist/js/sohoxi"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "chalk": "^1.1.3",
    "connect-multiparty": "^2.0.0",
    "directory-to-s3": "^1.0.0-rc4",
    "documentation": "^5.3.5",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.13.4",
    "express-slash": "^2.0.1",
    "extend": "^3.0.0",
    "grunt": "^1.0.1",
    "grunt-banner": "^0.6.0",
    "grunt-bump": "^0.8.0",
    "grunt-chokidar": "^1.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compress": "^1.3.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.2",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-eslint": "^20.1.0",
    "grunt-exec": "^3.0.0",
    "grunt-mocha-cli": "^3.0.0",
    "grunt-nodemon": "^0.4.2",
    "grunt-sass": "^2.0.0",
    "grunt-shell": "^2.1.0",
    "grunt-strip-code": "^1.0.4",
    "html-loader": "^0.5.5",
    "intern": "^3.4.6",
    "jasmine-core": "^2.9.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jsdom": "^9.9.1",
    "jshint": "^2.9.2",
    "karma": "^2.0.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^2.0.9",
    "load-grunt-tasks": "^3.5.2",
    "mmm": "^0.2.2",
    "moment": "^2.19.2",
    "node-pandoc": "^0.3.0",
    "pandoc": "^0.2.0",
    "phantomjs": "^2.1.7",
    "protractor": "^5.2.2",
    "requirejs": "^2.2.0",
    "rollup": "^0.51.7",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "selenium-standalone": "^6.12.0",
    "spec-xunit-file": "0.0.1-3",
    "unoconv": "^0.1.2",
    "vinyl-write": "^0.2.2",
    "webpack": "^4.0.0-beta.0",
    "webpack-cli": "^2.0.4"
  }
}
