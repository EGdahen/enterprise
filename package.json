{
  "name": "@infor/sohoxi",
  "version": "4.0.7",
  "shortName": "sohoxi",
  "description": "Hook & Loop - SoHo XI Controls ",
  "author": "Hook & Loop",
  "publishConfig": {
    "scope": "@infor",
    "registry": "http://npm.infor.com:4873/"
  },
  "scripts": {
    "start": "node server.js",

    "eslint": "eslint src -f node_modules/eslint-bamboo-formatter/reporter.js",
    "build": "grunt",

    "install-test-deps": "npm install selenium-standalone && npm run install-drivers",
      "install-drivers": "./node_modules/.bin/selenium-standalone install --version=2.47.1 --baseURL=http://selenium-release.storage.googleapis.com --drivers.chrome.version=2.18 --drivers.chrome.baseURL=http://chromedriver.storage.googleapis.com --drivers.ie.arch=x64 --drivers.ie.baseURL=http://selenium-release.storage.googleapis.com",
    "selenium": "./node_modules/.bin/selenium-standalone start",
    "test": "(npm run selenium & wait 3s) && ./node_modules/.bin/intern-runner config=test2/intern.local.functional"

  },
  "main": "sohoxi.js",
  "dependencies": {
  },
  "devDependencies": {
    "express": "^3.4.8",
    "chai": "^1.9.1",
    "chai-jquery": "^1.2.3",
    "colors": "^0.6.2",
    "connect-multiparty": "^2.0.0",
    "grunt": "~0.4.2",
    "grunt-banner": "^0.3.1",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.4.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-cssmin": "^0.11.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-uglify": "^0.8.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-git-revision": "0.0.1",
    "grunt-mocha-cli": "^1.9.0",
    "grunt-nodemon": "^0.4.0",
    "grunt-run": "^0.3.0",
    "grunt-sass": "^1.0.0",
    "grunt-shell": "^1.0.0",
    "grunt-strip-code": "^0.1.2",
    "intern": "^3.0.6",
    "jquery": "^3.0.0-alpha1",
    "jsdom": "^3.1.2",
    "jshint": "^2.5.5",
    "load-grunt-tasks": "^0.6.0",
    "mmm": "^0.2.2",
    "mocha": "^1.18.2",
    "phantomjs": "^1.9.17",
    "requirejs": "^2.1.19",
    "selenium-chromedriver": "0.0.2",
    "spec-xunit-file": "0.0.1-3",
